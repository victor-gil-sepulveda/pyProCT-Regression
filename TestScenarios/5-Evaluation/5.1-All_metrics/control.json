{
    "global": {
        "control": {
            "scheduler_type": "Process/Parallel",
            "number_of_processes": 6
        },
        "workspace": {
            "base": "clustering"
        }
    },
    "data": {
        "type": "protein::ensemble",
        "files": [
                "Data/files/9_natural_clusters.pdb"
        ],
        "matrix": {
            "method": "rmsd::ensemble",
            "parameters": {
                "calculator_type": "QCP_OMP_CALCULATOR",
                "fit_selection": "name CA"
            }
        }
    },
    "clustering": {
        "generation": {
            "method": "generate"
        },
        "algorithms": {
            "kmedoids": {
                "max": 15,
                "parameters":[{"k":4}]
            }
        },
        "evaluation": {
            "maximum_noise": 0,
            "evaluation_criteria": {
                "criteria_0": {
                    "Cohesion": {
                        "action": ">",
                        "weight": 1
                    }
                }
            },
            "minimum_cluster_size": 1,
            "query_types": [
                "NumClusters",
                "NoiseLevel",
                "MeanClusterSize",
                "Details",
                "NumClusteredElems",
                "PercentInTop4",
                "PercentInTop",
                "ClustersTo90",
                "Cohesion",
                "Compactness",
                "Separation",
                "Silhouette",
                "GaussianSeparation",
                "Calinski-Harabasz",
                "Dunn",
                "Davies-Bouldin",
                "RatioCut",
                "NCut",
                "MinMaxCut",
                "PCAanalysis"
        ],
            "maximum_clusters": 10,
            "minimum_clusters": 1
        }
    },
    "postprocess": {
        "representatives": {
            "keep_remarks": "ALL",
            "add_source_details": true
        },
        "pdb_clusters": {
            "keep_remarks": "ALL",
            "add_source_details": true
        },
        "cluster_stats":{}
    }
}
